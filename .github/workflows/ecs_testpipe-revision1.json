{
    "family": "ecs_testpipe",
    "containerDefinitions": [
        {
            "name": "testpipe",
            "image": "030925350335.dkr.ecr.us-east-1.amazonaws.com/rep_test_pipe:latest",
            "cpu": 0,
            "portMappings": [
                {
                    "name": "testpipe-80-tcp",
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true,
            "environment": [],
            "environmentFiles": [],
            "mountPoints": [],
            "volumesFrom": [],
            "ulimits": [],
